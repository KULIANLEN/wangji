
.content {
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	height: 80vh
}
.tabbar{
	display: flex;
	justify-content: space-around;
	height: 10vh;
	background-color: #8f8f94;
}
.icon{
	display: flex;
	width: 15vw;
	height: 15vw;
}
.logo {
	height: 200rpx;
	width: 200rpx;
	margin-top: 200rpx;
	margin-left: auto;
	margin-right: auto;
	margin-bottom: 50rpx;
}
.row-container{
	display: flex;
	flex-direction: column;
	justify-content: center;
}
.text-area {
	display: flex;
	justify-content: center;
}
.title {
	font-size: 36rpx;
	color: #8f8f94;
}

