}
.rel-vid{
    display: flex;
    flex-direction: row;
    height: 13vh;
    padding: 1vh;
    border: 1px;
    border-style: solid;
    border-color: gray;
}
.rel-vid image{
    width: 15vh;
    height: 10vh;
}
