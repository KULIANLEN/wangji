}
.image-wrap{
    display: flex;
    width: 100%;
    justify-content: center;
    margin-bottom: 5vh;
}
.extra{
    height:100vh;
}
.comments{
    overflow: scroll;
    height:1vh;
}
.comments::-webkit-scrollbar{
    display:none;
}
.relative{
    overflow: scroll;
    height:1vh
}
.relative::-webkit-scrollbar{
